{
  "project_info": {
    "project_number": "$TOGGL_GCM_SENDER_ID",
    "firebase_url": "$TOGGL_DATABASE_URL",
    "project_id": "$TOGGL_PROJECT_ID",
    "storage_bucket": "$TOGGL_STORAGE_BUCKET"
