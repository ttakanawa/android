          "package_name": "com.toggl"
        }
      },
      "oauth_client": [
        {
          "client_id": "$TOGGL_DROID_GOOGLE_SERVICES_CLIENT_ID",
          "client_type": 3
        }
      ],
      "api_key": [
        {
          "current_key": "$TOGGL_DROID_GOOGLE_SERVICES_API_KEY"
        }
      ],
      "services": {
        "analytics_service": {
          "status": 1
        },
        "appinvite_service": {
          "status": 1,
          "other_platform_oauth_client": []
        },
        "ads_service": {
          "status": 2
        }
      }
    },
    {
      "client_info": {
        "mobilesdk_app_id": "$TOGGL_DROID_DEBUG_GOOGLE_SERVICES_MOBILE_SDK_APP_ID_STAGING",
