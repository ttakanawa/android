        }
      },
      "oauth_client": [],
      "api_key": [
        {
          "current_key": "$TOGGL_DROID_GOOGLE_SERVICES_API_KEY"
        }
      ],
      "services": {
        "analytics_service": {
          "status": 1
        },
        "appinvite_service": {
          "status": 2,
          "other_platform_oauth_client": []
        },
        "ads_service": {
          "status": 2
        }
      }
    },
    {
      "client_info": {
        "mobilesdk_app_id": "$TOGGL_DROID_ADHOC_GOOGLE_SERVICES_MOBILE_SDK_APP_ID_STAGING",
        "android_client_info": {
          // the package_name should be updated to com.toggl.giskard.adhoc to match the current production app
          "package_name": "com.toggl.adhoc"
        }
      },
      "oauth_client": [],
      "api_key": [
        {
          "current_key": "$TOGGL_DROID_GOOGLE_SERVICES_API_KEY"
